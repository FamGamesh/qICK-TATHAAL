<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView
        android:layout_gravity="center"
        android:id="@id/card_view"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        card_view:cardElevation="10.0dip"
        card_view:cardUseCompatPadding="true">
        <RelativeLayout
            android:paddingBottom="20.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="20.0sp"
                android:textStyle="bold"
                android:textColor="#ffff4500"
                android:id="@id/textView36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginTop="7.0dip"
                android:text="@string/internet_banking"
                android:layout_toRightOf="@id/imageView"
                android:layout_alignBaseline="@id/imageView"
                android:layout_marginStart="10.0dip"
                android:layout_toEndOf="@id/imageView" />
            <ImageView
                android:id="@id/imageView"
                android:layout_width="24.0dip"
                android:layout_height="24.0dip"
                android:layout_marginLeft="10.0dip"
                android:layout_marginTop="10.0dip"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="10.0dip"
                android:layout_alignParentStart="true"
                card_view:srcCompat="@drawable/bullet" />
            <Spinner
                android:entries="@array/rc_netbanking"
                android:id="@id/spinner13"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5.0dip"
                android:layout_marginTop="15.0dip"
                android:layout_marginRight="10.0dip"
                android:layout_below="@id/textView36"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="5.0dip"
                android:layout_marginEnd="10.0dip"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true" />
            <TextView
                android:textSize="16.0sp"
                android:id="@id/textView21"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginTop="10.0dip"
                android:text="@string/sbi_bank_name"
                android:layout_below="@id/spinner13"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="10.0dip"
                android:layout_alignParentStart="true" />
            <Spinner
                android:entries="@array/sbiBankType"
                android:id="@id/spinner14"
                android:visibility="gone"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15.0dip"
                android:layout_toRightOf="@id/textView21"
                android:layout_alignRight="@id/editText22"
                android:layout_alignBottom="@id/textView21"
                android:layout_marginStart="15.0dip"
                android:layout_toEndOf="@id/textView21"
                android:layout_alignEnd="@id/editText22" />
            <TextView
                android:textSize="16.0sp"
                android:id="@id/textView22"
                android:paddingBottom="30.0dip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginTop="10.0dip"
                android:text="@string/banking_type"
                android:layout_below="@id/textView21"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="10.0dip"
                android:layout_alignParentStart="true" />
            <Spinner
                android:entries="@array/netBankingType"
                android:id="@id/spinner15"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_toRightOf="@id/textView22"
                android:layout_alignTop="@id/textView22"
                android:layout_alignRight="@id/spinner13"
                android:layout_marginStart="10.0dip"
                android:layout_toEndOf="@id/textView22"
                android:layout_alignEnd="@id/spinner13" />
            <EditText
                android:textColor="#ff008800"
                android:id="@id/editText22"
                android:background="@drawable/edit_drawable"
                android:paddingLeft="10.0dip"
                android:paddingTop="3.0dip"
                android:paddingRight="10.0dip"
                android:paddingBottom="3.0dip"
                android:visibility="gone"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginRight="10.0dip"
                android:layout_marginBottom="5.0dip"
                android:hint="@string/corporate_id"
                android:ems="10"
                android:layout_below="@id/textView22"
                android:layout_alignParentLeft="true"
                android:inputType="textPersonName"
                android:paddingStart="10.0dip"
                android:paddingEnd="10.0dip"
                android:layout_marginStart="10.0dip"
                android:layout_marginEnd="10.0dip"
                android:layout_alignParentStart="true" />
            <EditText
                android:textColor="#ff008800"
                android:id="@id/editText23"
                android:background="@drawable/edit_drawable"
                android:paddingLeft="10.0dip"
                android:paddingTop="3.0dip"
                android:paddingRight="10.0dip"
                android:paddingBottom="3.0dip"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginRight="10.0dip"
                android:hint="@string/username"
                android:ems="10"
                android:layout_below="@id/editText22"
                android:inputType="textPersonName"
                android:paddingStart="10.0dip"
                android:paddingEnd="10.0dip"
                android:layout_marginStart="10.0dip"
                android:layout_marginEnd="10.0dip" />
            <EditText
                android:textColor="#ff008800"
                android:id="@id/editText24"
                android:background="@drawable/edit_drawable"
                android:paddingLeft="10.0dip"
                android:paddingTop="3.0dip"
                android:paddingRight="10.0dip"
                android:paddingBottom="3.0dip"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5.0dip"
                android:hint="@string/password"
                android:ems="10"
                android:layout_below="@id/editText23"
                android:layout_alignLeft="@id/editText23"
                android:layout_alignRight="@id/editText23"
                android:layout_centerVertical="true"
                android:inputType="textPassword"
                android:paddingStart="10.0dip"
                android:paddingEnd="10.0dip"
                android:layout_alignStart="@id/editText23"
                android:layout_alignEnd="@id/editText23" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
</RelativeLayout>