<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:paddingBottom="10.0dip"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:animateLayoutChanges="true"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@id/divider"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5.0dip"
        android:layout_marginTop="10.0dip"
        android:layout_marginRight="5.0dip"
        android:src="@drawable/divider"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5.0dip"
        android:layout_marginEnd="5.0dip" />
    <ImageView
        android:id="@id/imageView2"
        android:layout_width="20.0dip"
        android:layout_height="20.0dip"
        android:layout_marginLeft="5.0dip"
        android:layout_marginTop="5.0dip"
        android:layout_marginRight="5.0dip"
        android:layout_below="@id/divider"
        android:layout_alignParentRight="true"
        android:layout_marginStart="5.0dip"
        android:layout_marginEnd="5.0dip"
        android:layout_alignParentEnd="true"
        app:srcCompat="@drawable/close" />
    <TextView
        android:textSize="16.0sp"
        android:id="@id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:text="@string/name"
        android:layout_alignBaseline="@id/editText6"
        android:layout_alignBottom="@id/editText6"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="10.0dip"
        android:layout_alignParentStart="true" />
    <EditText
        android:textColor="#ff008800"
        android:id="@id/editText6"
        android:background="@drawable/edit_drawable"
        android:paddingLeft="10.0dip"
        android:paddingTop="3.0dip"
        android:paddingRight="10.0dip"
        android:paddingBottom="3.0dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:layout_marginRight="10.0dip"
        android:ems="10"
        android:maxLength="16"
        android:layout_toLeftOf="@id/imageView2"
        android:layout_below="@id/imageView2"
        android:inputType="textCapWords"
        android:paddingStart="10.0dip"
        android:paddingEnd="10.0dip"
        android:layout_marginEnd="10.0dip"
        android:layout_toStartOf="@id/imageView2" />
    <TextView
        android:textSize="16.0sp"
        android:id="@id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/age"
        android:layout_alignBaseline="@id/spinner10"
        android:layout_alignLeft="@id/textView9"
        android:layout_alignBottom="@id/spinner10"
        android:layout_alignStart="@id/textView9" />
    <TextView
        android:textSize="16.0sp"
        android:id="@id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12.0dip"
        android:text="@string/gender"
        android:layout_below="@id/textView11"
        android:layout_alignLeft="@id/textView11"
        android:layout_alignStart="@id/textView11" />
    <Spinner
        android:entries="@array/gender"
        android:id="@id/spinner4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView12"
        android:layout_alignBaseline="@id/textView12"
        android:layout_alignLeft="@id/spinner10"
        android:layout_alignRight="@id/spinner10"
        android:layout_alignBottom="@id/textView12"
        android:layout_toEndOf="@id/textView12"
        android:layout_alignStart="@id/spinner10"
        android:layout_alignEnd="@id/spinner10" />
    <Spinner
        android:entries="@array/age"
        android:id="@id/spinner10"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10.0dip"
        android:layout_toRightOf="@id/textView11"
        android:layout_below="@id/editText6"
        android:layout_alignLeft="@id/editText6"
        android:layout_alignRight="@id/editText6"
        android:layout_toEndOf="@id/textView11"
        android:layout_alignStart="@id/editText6"
        android:layout_alignEnd="@id/editText6" />
</RelativeLayout>