<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@drawable/theme_round_dialog"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="vertical"
        android:id="@id/linearLayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="22.0sp"
            android:textStyle="bold"
            android:textColor="#ffbbbbbb"
            android:layout_gravity="center"
            android:id="@id/textView48"
            android:layout_width="wrap_content"
            android:layout_height="0.0dip"
            android:layout_marginTop="10.0dip"
            android:layout_marginBottom="10.0dip"
            android:text="@string/info"
            android:layout_weight="1.0" />
    </LinearLayout>
    <RelativeLayout
        android:id="@id/linearLayout5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/linearLayout"
        android:layout_alignParentStart="true">
        <TextView
            android:textSize="14.0sp"
            android:textStyle="bold"
            android:textColor="#ffd1a700"
            android:id="@id/textView72"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30.0dip"
            android:text="You have active GOLD Pack subscription"
            android:layout_centerInParent="true"
            android:paddingStart="10.0dip"
            android:paddingEnd="10.0dip"
            android:layout_marginStart="10.0dip"
            android:layout_marginEnd="10.0dip" />
        <TextView
            android:textSize="12.0sp"
            android:textColor="#ffaaaaaa"
            android:id="@id/expInfo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textView72"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="10.0dip"
            android:layout_marginEnd="10.0dip" />
    </RelativeLayout>
    <Button
        android:textSize="18.0sp"
        android:textColor="#ffffffff"
        android:id="@id/proceed"
        android:background="@drawable/new_form_bg"
        android:visibility="visible"
        android:layout_width="200.0dip"
        android:layout_height="40.0dip"
        android:layout_margin="20.0dip"
        android:layout_marginTop="10.0dip"
        android:text="@string/ok"
        android:layout_below="@id/linearLayout5"
        android:layout_centerInParent="true" />
</RelativeLayout>