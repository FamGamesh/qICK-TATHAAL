<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:id="@id/scrollView"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            android:layout_gravity="center"
            android:id="@id/card_view"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            card_view:cardElevation="10.0dip"
            card_view:cardUseCompatPadding="true">
            <RelativeLayout
                android:paddingBottom="20.0dip"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TextView
                    android:textSize="20.0sp"
                    android:textStyle="bold"
                    android:textColor="#ffff4500"
                    android:id="@id/textView36"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="7.0dip"
                    android:text="@string/miscellaneaous"
                    android:layout_toRightOf="@id/imageView"
                    android:layout_alignBaseline="@id/imageView"
                    android:layout_marginStart="10.0dip"
                    android:layout_toEndOf="@id/imageView" />
                <ImageView
                    android:id="@id/imageView"
                    android:layout_width="24.0dip"
                    android:layout_height="24.0dip"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="10.0dip"
                    android:layout_alignParentStart="true"
                    card_view:srcCompat="@drawable/bullet" />
                <CheckBox
                    android:textSize="16.0sp"
                    android:id="@id/checkBox4"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/opted_for_vikalp"
                    android:layout_below="@id/imageView"
                    android:layout_alignParentLeft="true"
                    android:layout_marginStart="10.0dip"
                    android:layout_alignParentStart="true" />
                <CheckBox
                    android:textSize="16.0sp"
                    android:id="@id/checkBox6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/auto_upgradation"
                    android:layout_below="@id/imageView"
                    android:layout_alignLeft="@id/checkBox4"
                    android:layout_marginStart="10.0dip"
                    android:layout_alignStart="@id/checkBox4" />
                <CheckBox
                    android:textSize="16.0sp"
                    android:id="@id/checkBox7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/only_confirm_berths"
                    android:layout_below="@id/checkBox6"
                    android:layout_alignLeft="@id/checkBox6"
                    android:layout_alignStart="@id/checkBox6" />
                <TextView
                    android:textSize="16.0sp"
                    android:textStyle="bold"
                    android:textColor="#ff0055ff"
                    android:id="@id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5.0dip"
                    android:text="@string/travel_insurance"
                    android:layout_below="@id/checkBox7"
                    android:layout_alignLeft="@id/checkBox7"
                    android:layout_alignStart="@id/checkBox7" />
                <RadioGroup
                    android:orientation="horizontal"
                    android:id="@id/radioGroup1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.0dip"
                    android:layout_toRightOf="@id/textView17"
                    android:layout_below="@id/checkBox7"
                    android:layout_marginStart="8.0dip"
                    android:layout_toEndOf="@id/textView17">
                    <RadioButton
                        android:textStyle="bold"
                        android:id="@id/yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:text="Yes"
                        android:layout_weight="1.0" />
                    <RadioButton
                        android:textStyle="bold"
                        android:id="@id/no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15.0dip"
                        android:text="No"
                        android:layout_weight="1.0"
                        android:layout_marginStart="15.0dip" />
                </RadioGroup>
                <TextView
                    android:textSize="16.0sp"
                    android:textStyle="bold"
                    android:textColor="#ffff4500"
                    android:id="@id/textView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15.0dip"
                    android:text="@string/booking_option"
                    android:layout_below="@id/textView17"
                    android:layout_alignLeft="@id/checkBox7"
                    android:layout_marginStart="5.0dip"
                    android:layout_marginEnd="5.0dip"
                    android:layout_alignStart="@id/checkBox7" />
                <RadioGroup
                    android:id="@id/radioGroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:weightSum="1.0"
                    android:layout_below="@id/textView15"
                    android:layout_alignLeft="@id/textView15"
                    android:layout_marginStart="20.0dip"
                    android:layout_marginEnd="10.0dip"
                    android:layout_alignStart="@id/textView15">
                    <RadioButton
                        android:textSize="16.0sp"
                        android:id="@id/radioButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:text="@string/none" />
                    <RadioButton
                        android:textSize="16.0sp"
                        android:id="@id/radioButton2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/booking_opt_1" />
                    <RadioButton
                        android:textSize="16.0sp"
                        android:id="@id/radioButton3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/booking_opt_2" />
                    <RadioButton
                        android:textSize="16.0sp"
                        android:id="@id/radioButton4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/booking_opt_3" />
                </RadioGroup>
                <CheckBox
                    android:textSize="16.0sp"
                    android:id="@id/checkBox8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="5.0dip"
                    android:text="@string/coach_preferred"
                    android:layout_below="@id/radioGroup"
                    android:layout_alignParentLeft="true"
                    android:layout_marginStart="10.0dip"
                    android:layout_alignParentStart="true" />
                <EditText
                    android:enabled="false"
                    android:textColor="#ff008800"
                    android:id="@id/editText13"
                    android:background="@drawable/edit_drawable_disabled"
                    android:paddingLeft="10.0dip"
                    android:paddingTop="3.0dip"
                    android:paddingRight="10.0dip"
                    android:paddingBottom="3.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:hint="@string/coach_id"
                    android:ems="10"
                    android:layout_toRightOf="@id/checkBox8"
                    android:layout_alignTop="@id/checkBox8"
                    android:layout_alignParentRight="true"
                    android:inputType="textPersonName"
                    android:paddingStart="10.0dip"
                    android:paddingEnd="10.0dip"
                    android:layout_marginStart="10.0dip"
                    android:layout_marginEnd="10.0dip"
                    android:layout_toEndOf="@id/checkBox8"
                    android:layout_alignParentEnd="true" />
                <TextView
                    android:textSize="16.0sp"
                    android:id="@id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30.0dip"
                    android:text="@string/mobile_no"
                    android:layout_below="@id/editText13"
                    android:layout_alignLeft="@id/checkBox8"
                    android:layout_alignStart="@id/checkBox8" />
                <EditText
                    android:textColor="#ff008800"
                    android:id="@id/editText15"
                    android:background="@drawable/edit_drawable"
                    android:paddingLeft="10.0dip"
                    android:paddingTop="3.0dip"
                    android:paddingRight="10.0dip"
                    android:paddingBottom="3.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:ems="10"
                    android:maxLength="10"
                    android:layout_toRightOf="@id/textView16"
                    android:layout_alignBaseline="@id/textView16"
                    android:layout_alignRight="@id/checkBox7"
                    android:layout_alignBottom="@id/textView16"
                    android:inputType="number"
                    android:paddingStart="10.0dip"
                    android:paddingEnd="10.0dip"
                    android:layout_marginStart="10.0dip"
                    android:layout_toEndOf="@id/textView16"
                    android:layout_alignEnd="@id/checkBox7" />
                <TextView
                    android:textSize="16.0sp"
                    android:textStyle="bold"
                    android:textColor="#ff0055ff"
                    android:id="@id/pmTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/mode"
                    android:layout_below="@id/editText15"
                    android:layout_marginStart="10.0dip" />
                <Spinner
                    android:entries="@array/payment_mode"
                    android:id="@id/paymentMode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10.0dip"
                    android:layout_below="@id/editText15"
                    android:layout_marginStart="10.0dip"
                    android:layout_toEndOf="@id/pmTv" />
                <CheckBox
                    android:textStyle="bold"
                    android:id="@id/autofillCaptcha"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5.0dip"
                    android:text="Autofill Captchas"
                    android:layout_below="@id/paymentMode"
                    android:layout_marginStart="5.0dip" />
                <ImageView
                    android:padding="5.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/new_label"
                    android:scaleType="fitStart"
                    android:layout_alignTop="@id/autofillCaptcha"
                    android:layout_alignBottom="@id/autofillCaptcha"
                    android:layout_marginStart="5.0dip"
                    android:layout_toEndOf="@id/autofillCaptcha" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </RelativeLayout>
</ScrollView>