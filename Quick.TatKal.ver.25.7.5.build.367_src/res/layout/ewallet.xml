<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@color/primary_color"
    android:padding="10.0dip"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">
    <TextView
        android:textSize="20.0sp"
        android:textStyle="bold"
        android:textColor="#ffffffff"
        android:id="@id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cash_card_wallets"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10.0dip" />
    <RelativeLayout
        android:background="@drawable/gradient_rectangle"
        android:paddingBottom="20.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15.0dip"
        android:layout_below="@id/textView36">
        <TextView
            android:textSize="13.0sp"
            android:textColor="#ffffffff"
            android:id="@id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.0dip"
            android:text="@string/e_wallet"
            android:layout_marginStart="30.0dip"
            android:layout_alignParentStart="true" />
        <Spinner
            android:entries="@array/wallet"
            android:id="@id/spinner17"
            android:background="@drawable/spinner_theme"
            android:layout_width="wrap_content"
            android:layout_height="40.0dip"
            android:layout_marginTop="5.0dip"
            android:layout_below="@id/textView32"
            android:layout_marginStart="20.0dip"
            android:layout_marginEnd="20.0dip"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true" />
        <TextView
            android:textSize="13.0sp"
            android:textColor="#ffffffff"
            android:id="@id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15.0dip"
            android:text="@string/mobile_no_username"
            android:layout_below="@id/spinner17"
            android:layout_alignStart="@id/textView32" />
        <EditText
            android:textColor="#ffffffff"
            android:id="@id/editText27"
            android:background="@drawable/free_tickets_bg"
            android:paddingLeft="10.0dip"
            android:paddingTop="3.0dip"
            android:paddingRight="10.0dip"
            android:paddingBottom="3.0dip"
            android:layout_width="wrap_content"
            android:layout_height="40.0dip"
            android:layout_marginTop="5.0dip"
            android:ems="10"
            android:layout_below="@id/textView33"
            android:inputType="textPersonName"
            android:paddingStart="10.0dip"
            android:paddingEnd="10.0dip"
            android:layout_alignStart="@id/spinner17"
            android:layout_alignEnd="@id/spinner17" />
        <TextView
            android:textSize="13.0sp"
            android:textColor="#ffffffff"
            android:id="@id/textView34"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15.0dip"
            android:text="@string/password"
            android:layout_below="@id/editText27"
            android:layout_alignStart="@id/textView33"
            android:layout_alignEnd="@id/textView33" />
        <EditText
            android:textColor="#ffffffff"
            android:id="@id/editText34"
            android:background="@drawable/free_tickets_bg"
            android:paddingLeft="10.0dip"
            android:paddingTop="3.0dip"
            android:paddingRight="10.0dip"
            android:paddingBottom="3.0dip"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="40.0dip"
            android:layout_marginTop="5.0dip"
            android:ems="10"
            android:layout_below="@id/textView34"
            android:inputType="textPassword"
            android:paddingStart="10.0dip"
            android:paddingEnd="10.0dip"
            android:layout_alignStart="@id/spinner17"
            android:layout_alignEnd="@id/spinner17" />
    </RelativeLayout>
</RelativeLayout>