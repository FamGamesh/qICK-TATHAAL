<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@drawable/theme_round_dialog"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="vertical"
        android:id="@id/linearLayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="22.0sp"
            android:textStyle="bold"
            android:textColor="#ffffffff"
            android:layout_gravity="center"
            android:id="@id/textView48"
            android:layout_width="wrap_content"
            android:layout_height="0.0dip"
            android:layout_marginTop="10.0dip"
            android:layout_marginBottom="10.0dip"
            android:text="@string/form_name"
            android:layout_weight="1.0" />
    </LinearLayout>
    <RelativeLayout
        android:id="@id/linearLayout5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/linearLayout">
        <EditText
            android:textSize="16.0sp"
            android:textColor="#ffffffff"
            android:id="@id/editText38"
            android:background="@drawable/custom_edittext_line"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30.0dip"
            android:layout_centerInParent="true"
            android:inputType="textCapWords"
            android:paddingStart="10.0dip"
            android:layout_marginStart="20.0dip"
            android:layout_marginEnd="20.0dip" />
        <TextView
            android:textStyle="bold"
            android:textColor="#ffd83237"
            android:id="@id/textView44"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:text="Error"
            android:layout_below="@id/editText38"
            android:layout_alignStart="@id/editText38" />
    </RelativeLayout>
    <Button
        android:textSize="18.0sp"
        android:textColor="#ffffffff"
        android:id="@id/button"
        android:background="@drawable/new_form_bg"
        android:visibility="visible"
        android:layout_width="200.0dip"
        android:layout_height="40.0dip"
        android:layout_margin="20.0dip"
        android:text="@string/submit"
        android:layout_below="@id/linearLayout5"
        android:layout_centerInParent="true"
        android:textAllCaps="false" />
</RelativeLayout>