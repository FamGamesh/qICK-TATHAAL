<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="@style/Theme.AppCompat">
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>
    <style name="AppTheme.AdAttribution" parent="@style/AppTheme">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:background">#ffffcc66</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">Ad</item>
        <item name="android:height">15.0dip</item>
        <item name="android:width">15.0dip</item>
    </style>
    <style name="CustomActionBarTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:spinnerItemStyle">@style/TextViewSpinnerItem</item>
        <item name="android:popupTheme">@style/CustomSpinnerTheme</item>
        <item name="colorPrimary">@color/apptheme_color</item>
        <item name="colorPrimaryDark">@color/apptheme_color_dark</item>
        <item name="popupTheme">@style/CustomSpinnerTheme</item>
    </style>
    <style name="customDialog" parent="@style/Theme.AppCompat.Light.Dialog">
        <item name="android:windowNoTitle">false</item>
    </style>
</resources>
